<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 5.3//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd"> 
<hibernate-mapping>
<class name="com.modal.Query1" table="query1">
<id name="id" type="int">  
<generator class="increment"></generator>  
</id> 
<property name="query" type="string" not-null="true"  />
<property name="answer" type="string" />
<property name="subject" type="string" length="200" not-null="true"/>
<property name="student" type="string" length="500" not-null="true"/>
<property name="faculty" type="string" length="500" not-null="true"/>
<property name="date" type="date"/>
</class>
</hibernate-mapping>

